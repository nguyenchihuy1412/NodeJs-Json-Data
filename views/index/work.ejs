
<!-- Work Section Begin -->
<section class="work">
    <div class="work__gallery">
        <div class="grid-sizer"></div>
        <% dataJson.video.forEach(function(video) { %>
        <div class="work__item wide__item set-bg" data-setbg="/images/work/<%= video.bg %>">
            <a href="<%= video.source %>" class="play-btn video-popup"><i
                    class="fa fa-play"></i></a>
            <div class="work__item__hover">
                <h4>VIP Auto Tires & Service</h4>
                <ul>
                    <li>eCommerce</li>
                    <li>Magento</li>
                </ul>
            </div>
        </div>
        <% }) %>
    </div>
</section>
<!-- Work Section End -->