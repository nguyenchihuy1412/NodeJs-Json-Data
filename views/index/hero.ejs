<!-- Hero Section Begin -->
<section class="hero">
    <div class="hero__slider owl-carousel">
        <% dataJson.slide.forEach(function(slide){ %>
        <div class="hero__item set-bg" data-setbg="/images/hero/hero-1.jpg">
            <div class="container">
                <div class="row">
                    
                    <div class="col-lg-6">
                        <div class="hero__text">
                            
                            <span><%= slide.title %></span>
                            <h2><%= slide.header %></h2>
                            
                            <a href="#" class="primary-btn">See more about us</a>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
        <% }) %>
    </div>
</section>
<!-- Hero Section End -->
